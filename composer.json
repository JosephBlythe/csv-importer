{
  "name": "csv-importer/app",
  "description": "CSV User Importer Application",
  "type": "project",
  "require": {
    "php": "^8.3",
    "ext-pdo": "*",
    "ext-pdo_pgsql": "*",
    "vlucas/phpdotenv": "^5.6",
    "symfony/console": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable"
}
